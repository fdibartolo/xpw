<h1>New Job Offer</h1>
<% form_for(@job_offer, '/job_offers/create') do |f| %>

  <%= partial 'job_offers/form', :locals => { :f => f } %>

  <%= f.submit "Create", :class => "btn btn-primary" %>
  <%= submit_tag "Create and Twitter", :class => "btn btn-primary", :name => 'to_twitter' %>
  <%= link_to 'Cancel', url_for(:job_offers, :my), :class => "btn btn-secondary" %>
<% end %>